{
  "name": "react-apollo-github-top-js-repos",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://dipiash.github.io/react-apollo-github-top-js-repos",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-app-rewired test --coverage --watchAll=false --forceExit",
    "test:ci": "react-app-rewired test --runInBand --coverage --watchAll=false --reporters=default --reporters=jest-junit --coverageReporters=text-lcov > ./coverage/lcov.info",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "eject": "react-app-rewired eject",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!/node_modules/",
      "!/public/",
      "!src/serviceWorker.js",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@rehooks/online-status": "^1.1.2",
    "apollo-boost": "^0.4.9",
    "classnames": "^2.3.1",
    "gh-pages": "^3.2.3",
    "graphql": "^15.6.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-app-rewired": "^2.1.8",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "use-debounce": "^7.0.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest-junit": "^13.0.0",
    "prettier": "^2.4.1"
  }
}
